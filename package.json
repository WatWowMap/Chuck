  "dependencies": {
    "@turf/turf": "^6.5.0",
    "axios": "^0.21.4",
    "better-logging": "^4.5.0",
    "eslint": "^7.32.0",
    "express": "^4.17.1",
    "extend": "^3.0.2",
    "google-protobuf": "^3.18.0",
    "helmet": "^4.6.0",
    "i18n": "^0.13.3",
    "lru-cache": "^6.0.0",
    "mustache-express": "^1.3.2",
